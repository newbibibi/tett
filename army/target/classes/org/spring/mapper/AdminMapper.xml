<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.spring.mapper.AdminMapper">
	<update id="answerQ" parameterType="QuestionsVO">
		UPDATE questions
		SET answer=#{answer}
		WHERE qno=#{qno}
	</update>

</mapper>